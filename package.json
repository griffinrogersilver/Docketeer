{
  "name": "docketeer",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A Docker Visualizer",
  "author": "Team Docketeer",
  "proxy": "http://localhost:3000",
  "main": "./src/main/main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently 'electron-webpack dev' 'nodemon server/server.js'",
    "windev": "cross-env NODE_ENV=developement electron-webpack dev",
    "server": "node server/server.js",
    "compile": "electron-webpack",
    "dist": "npm run compile && electron-builder",
    "dist-win": "npm run compile && electron-builder --win --x64",
    "dist:dir": "yarn dist  --dir -c.compression=store -c.mac.identity=null",
    "test": "jest --verbose",
    "temp": "webpack-dev-server --open --mode development"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.2",
    "chart.js": "^2.9.3",
    "clsx": "^1.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.2",
    "electron-chartjs": "^1.0.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-fetch": "^1.7.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "express": "^4.17.1",
    "fix-path": "^3.0.0",
    "jest": "^26.4.2",
    "node-fetch": "^2.6.1",
    "pg": "^8.4.1",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.10.0",
    "react-dom": "^16.13.1",
    "react-modal": "^3.14.2",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "sass-loader": "^10.0.2",
    "source-map-support": "^0.5.19",
    "style-loader": "^1.2.1",
    "twilio": "^3.50.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.2.1",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^6.2.0",
    "electron": "^10.1.0",
    "electron-builder": "^22.4.1",
    "electron-webpack": "^2.8.2",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.28.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "nodemon": "^2.0.7",
    "react-test-renderer": "^16.13.1",
    "redux-thunk": "^2.3.0",
    "webpack": "^4.44.1"
  },
  "electronWebpack": {
    "whiteListedModules": [
      "react-redux"
    ],
    "main": {
      "sourceDirectory": "src/main"
    },
    "renderer": {
      "sourceDirectory": "src/renderer",
      "template": "src/renderer/index.html"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/docketeer.git"
  },
  "build": {
    "extraResources": [
      {
        "from": "./src/database/",
        "to": "database",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
